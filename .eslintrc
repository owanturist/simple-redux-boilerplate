{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [ "react" ],
    "rules": {
        "newline-after-var": [2, "always"],
        "comma-dangle": 0,
        "indent": [2, 4],
        "react/no-multi-comp": 0,
        "import/default": 0,
        "import/no-duplicates": 0,
        "import/named": 0,
        "import/namespace": 0,
        "import/no-unresolved": 0,
        "block-scoped-var": 0,
        "padded-blocks": 0,
        "comma-dangle": 0,
        "no-console": 0,
        "no-alert": 0,
        "eqeqeq": 0,
        "no-var": 0,
        "react/jsx-uses-react": 1,
        "react/jsx-no-bind": 0,
        "react/prop-types": 1,
        "react/jsx-indent-props": [2, 4],
        "array-bracket-spacing": 0
    },
    "globals": {
        "__DEVELOPMENT__": true
    }
}
